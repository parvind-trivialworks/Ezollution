@model EzollutionPro_BAL.Models.ClientManagementSearchModel
@{
    /**/

    ViewBag.Title = "Client Management";
    Layout = "~/Views/Shared/_Layout.cshtml";

   
}
<link href="~/Content/datepicker/datepicker3.css" rel="stylesheet" />
<link href="~/Content/daterangepicker/daterangepicker.css" rel="stylesheet" />

<div class="box box-primary">
    <div class="box-header with-border">
        <h3 class="box-title"><i class="fa fa-files-o"></i>&nbsp;@ViewBag.Title
        </h3>
        <div class="pull-right">
            <a href="/ClientManagement/AddEdit" class="btn btn-success"><i class="fa fa-plus"></i> Add New Client</a>
        </div>
       
    </div>
    <div class="box-body">
        <div class="row">
            <div id="home" class="tab-pane fade in active">
                <div class="">

                    <div class="box-body" style="min-height:500px">

                        @using (Html.BeginForm("Index", "ClientManagementModel", FormMethod.Post, new { @id = "frmClient" }))
                        {
                            <div class="row mt-3">
                                <div class="col-md-12">
                                    <div class="col-md-2">
                                        @Html.Label("From Date")
                                        @Html.TextBoxFor(x => x.FromDate, new { @class = "form-control" })

                                    </div>

                                    <div class="col-md-2">
                                        @Html.Label("To Date")
                                        @Html.TextBoxFor(x => x.ToDate, new { @class = "form-control" })

                                    </div>
                                    <div class="col-md-2">
                                        <label>&nbsp;</label>
                                        <div class="input-group">
                                            <input type="button" class="btn btn-info" id="btnSearchClientManagement" value="Search Clients" />
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <br />

                        }
                        <br />
                        <div class="table table-responsive"  style="overflow:scroll;overflow-y:hidden">
                            <table id="tblClientManagementList" class="table table-striped table-bordered" >
                                <thead>
                                    <tr>
                                        <th>SNo</th>
                                        <th>Client Code</th>
                                        <th>Date</th>
                                        <th>New Client Name</th>
                                        <th>Existing Client Name</th>
                                        <th>Contact Person Name</th>
                                        <th>Contact Person Contact No</th>
                                        <th>Decision Maker</th>
                                        <th>Decision Maker Contact No</th>
                                        <th>Reference</th>
                                        <th>Branch</th>
                                        <th>Address</th>
                                        <th>Next Follow UpDate</th>
                                        <th>EmailId</th>
                                        <th>Service Type</th>
                                        <th>Rate Offered</th>
                                        <th>Final Rate</th>
                                        <th>Revenue Expected</th>
                                        <th>Actual Revenue</th>
                                        <th>Business Start Date</th>
                                        <th>Create Date</th>
                                    </tr>
                                </thead>
                            </table>
                        </div>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>


@section scripts{
    @Scripts.Render("~/bundles/jqueryval")
    <script src="~/Scripts/plugins/moment/moment.min.js"></script>
    <script src="~/Content/datepicker/bootstrap-datepicker.js"></script>
    <script src="~/Scripts/plugins/daterangepicker/daterangepicker.js"></script>
    <script src="~/Scripts/PageScripts/ClientManagement.js"></script>
    <script>
        $(document).ready(function () {
            //$("#btnGet").click(function (event) {
               
            //});
        });
    </script>
    }


           
