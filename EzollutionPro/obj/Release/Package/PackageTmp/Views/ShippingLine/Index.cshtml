<div class="box box-primary">
    <div class="box-header with-border">
        <h3 class="box-title">Shipping Lines</h3>
        <div class="pull-right">
            <div class="btn-group">
                <button type="button" class="btn btn-success" onclick="AddShippingLine(0)"><i class="fa fa-plus"></i> Add New Shipping Line</button>
            </div>
        </div>
        <div class="pull-right">
        </div>
    </div>
    <div class="box-body">
        <div class="table table-responsive">
            <table class="table table-bordered" id="tblShippingLine">
                <thead>
                    <tr>
                        <th>SNo.</th>
                        <th>ShippingLine Name</th>
                        <th>MLO Code</th>
                        <th>Action</th>
                    </tr>
                </thead>
            </table>
        </div>
    </div>
</div>
@section scripts{
    @Scripts.Render("~/bundles/jqueryval")
    <script src="~/Scripts/PageScripts/Masters/ShippingLine.js"></script>
}

